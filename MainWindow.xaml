<Window x:Class="ManualWebScraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManualWebScraper"
        xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Top" Name="ScrapeButton" Click="ScrapeButton_Click" Height="30" Margin="5">Scrape</Button>
        <cefSharp:ChromiumWebBrowser x:Name="Browser" Address="https://21sextury.com"/>
    </DockPanel>
</Window>
